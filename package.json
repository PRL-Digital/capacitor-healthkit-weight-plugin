{
  "name": "ditchyourdiet",
  "version": "1.0.18",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-prod": "env-cmd -f .env.production yarn dev",
    "start": "next start",
    "build": "next build",
    "build:local": "env-cmd -f .env.local yarn build",
    "lint": "next lint",
    "static": "cross-env NEXT_STATIC=TRUE next build",
    "serve": "serve ./out",
    "story": "storybook dev -p 6006",
    "process-ai": "env-cmd -f .env.local-prod yarn dev",
    "build-storybook": "storybook build",
    "build-andr:local": "env-cmd -f .env.local yarn static && npx cap sync android && npx cap copy android && npx cap open android",
    "build-andr:prod": "env-cmd -f .env.production yarn static && npx cap sync android && npx cap copy android && npx cap open android",
    "build-ios:local": "env-cmd -f .env.local yarn static && npx cap sync ios && npx cap copy ios && npx cap open ios",
    "build-ios:prod": "env-cmd -f .env.production yarn static && npx cap sync ios && npx cap copy ios && npx cap open ios"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.1",
    "@capacitor/app": "^6.0.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/core": "^6.1.1",
    "@capacitor/device": "^6.0.0",
    "@capacitor/ios": "^6.1.1",
    "@capacitor/network": "^6.0.1",
    "@capacitor/share": "^6.0.1",
    "capacitor-healthkit": "file:capacitor-healthkit",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.1",
    "cross-env": "^7.0.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "stylelint": "^16.8.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "6.0.4",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.4.1"
  }
}
